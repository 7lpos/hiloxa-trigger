{
    "name": "hl/hiloxa-trigger",
    "description": "Webhook trigger package for hiloxa",
    "version": "1.0.0",
    "homepage": "https://github.com/husseinzaher/hiloxa-trigger",
    "authors": [
        {
            "name": "Hussein zaher",
            "email": "husseinzaher@outllok.com"
        }
    ],
    "keywords": [
        "hiloxa",
        "webhook",
        "laravel",
        "Webhook trigger",
        "Event"
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.2",
        "illuminate/database": "^11.0",
        "illuminate/queue": "^11.0",
        "illuminate/support": "^11.0",
        "illuminate/http": "^10.0|^11.0",
        "nesbot/carbon": "^3.0",
        "spatie/laravel-package-tools": "^1.16"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "orchestra/testbench": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "HL\\HiloxaTrigger\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes",
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "HL\\HiloxaTrigger\\HiloxaTriggerServiceProvider"
            ]
        }
    }
}
