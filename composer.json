{
    "name": "hl/hiloxa-trigger",
    "description": "Webhook trigger package for hiloxa",
    "version": "1.0.0",
    "homepage": "https://github.com/husseinzaher/hiloxa-trigger",
    "authors": [
        {
            "name": "Hussein zaher",
            "email": "husseinzaher@outllok.com"
        }
    ],
    "keywords": [
        "hiloxa",
        "webhook",
        "laravel",
        "Webhook trigger",
        "Event"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0|^8.1|^8.2",
        "illuminate/database": "^8.0|^9.0|^10.0",
        "illuminate/queue": "^8.0|^9.0|^10.0",
        "illuminate/support": "^8.0|^9.0|^10.0",
        "illuminate/http": "^8.0|^9.0|^10.0",
        "nesbot/carbon": "^2.53.1|^2.63",
        "spatie/laravel-package-tools": "^1.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "HL\\HiloxaTrigger\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes",
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "HL\\HiloxaTrigger\\HiloxaTriggerServiceProvider"
            ]
        }
    }
}
